{
  "name": "node-ts-pg-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/app.js",
  "nyc": {
    "exclude": [
	  "dist/test/**/*"
    ],
    "reporter": "lcov"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/src/app.js",
    "test": "cross-env NODE_ENV=test mocha --recursive dist/test --delay -r dotenv/config",
    "build": "tsc",
    "build-clean": "del-cli dist && npm run build",
    "restart": "npm run lint && npm run build-clean && npm start",
    "build-watch": "tsc --watch",
    "lint": "tslint --project tsconfig.json --config tslint.json --format verbose",
    "coverage": "nyc npm test",
    "view-coverage": "opn coverage/lcov-report/index.html"
  },
  "author": "Robert Helms <robert.helms@smashingboxes.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "connect-session-sequelize": "^5.2.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "del-cli": "^1.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "opn-cli": "^3.1.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/mocha": "^5.2.5",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.4.10",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
